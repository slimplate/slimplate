{
  "name": "@slimplate/react-flowbite-github",
  "version": "0.0.0",
  "type": "module",
  "dependencies": {
    "@slimplate/react-github": "file:packages/react-github",
    "@slimplate/utils": "file:packages/utils",
    "classnames": "^2.3.2",
    "flowbite": "^1.6.3",
    "flowbite-react": "^0.3.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tabler-icons-react": "^1.56.0",
    "template-templates": "^1.0.1"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.13",
    "microbundle": "^0.15.1",
    "postcss": "^8.4.21",
    "tailwindcss": "^3.2.7"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "microbundle --jsx jsx --jsxImportSource react --globals react/jsx-runtime=jsx"
